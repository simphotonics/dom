{
    "name": "simphotonics/dom",
    "type": "library",
    "description": "Create, edit, output XML nodes and documents.",
    "keywords": ["dom", "ml", "markup", "node", "tree", "xhtml"],
    "license": "MIT",
    "homepage": "http://github.com/simphotonics/dom",
    "support": {
        "issues": "https://github.com/simphotonics/dom/issues",
        "source": "https://github.com/simphotonics/dom"
    },
    "authors": [
        {
            "name": "D Reschner",
            "email": "git@simphotonics.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "simphotonics/utils":"^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Simphotonics\\Dom\\": "src/"
        }
    },
    "autoload-dev":{
        "psr-4": {
            "Simphotonics\\Dom\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
